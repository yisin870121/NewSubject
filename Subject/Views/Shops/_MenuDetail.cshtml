@model IEnumerable<Subject.Models.ShopMenu>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Item)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MenuDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Users.UserName)
            </th>
        </tr>
    </thead>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Item)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MenuDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Users.UserName)
            </td>
        </tr>
    }

</table>

